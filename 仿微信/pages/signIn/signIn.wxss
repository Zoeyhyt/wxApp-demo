/* pages/signIn/signIn.wxss */
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.checked-button{
  margin-top: 64rpx;
  color: black;
  font-size:16px;
}
.unchecked-button{
  background: #0e932e;
  margin-top: 64rpx;
  color: #fff;
  font-size: 16px;
}
.status{
  margin-top: 48rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.status-item{
  font-size: 0.8rem;
  color: #999;
  margin-bottom:16rpx; 
}
.result{
  color: #666;
  font-size: 0.9rem;
  margin-top: 48rpx;
}